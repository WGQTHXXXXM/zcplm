#
# Structure for table "materials"
#

DROP TABLE IF EXISTS `materials`;
CREATE TABLE `materials` (
  `material_id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT '主索引',
  `assy_level` tinyint(3) NOT NULL DEFAULT '0' COMMENT '装配等级',
  `purchase_level` varchar(10) NOT NULL DEFAULT '' COMMENT '采购等级',
  `mfr_part_number` varchar(255) NOT NULL DEFAULT '' COMMENT '厂商零件编号',
  `description` varchar(255) NOT NULL DEFAULT '' COMMENT '描述',
  `pcb_footprint` varchar(255) DEFAULT NULL COMMENT '封装',
  `manufacturer` int(11) NOT NULL DEFAULT '0' COMMENT '供应商',
  `zc_part_number` varchar(40) NOT NULL DEFAULT '' COMMENT '智车料号',
  `date_entered` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `vehicle_standard` tinyint(3) DEFAULT NULL COMMENT '物料级别',
  `part_type` int(11) NOT NULL COMMENT '零件类型',
  `value` varchar(255) DEFAULT NULL COMMENT '零件大小值',
  `schematic_part` varchar(255) DEFAULT '' COMMENT '零件原理图',
  `datasheet` varchar(255) DEFAULT NULL COMMENT '规格书',
  `price` varchar(20) DEFAULT NULL COMMENT '单价',
  `recommend_purchase` tinyint(3) NOT NULL DEFAULT '0' COMMENT '采购等级',
  `minimum_packing_quantity` int(11) DEFAULT NULL COMMENT '最小包装量',
  `lead_time` int(11) DEFAULT NULL COMMENT '交货周期',
  `manufacturer2_id` int(11) DEFAULT NULL COMMENT '二供的id号',
  `manufacturer3_id` int(11) DEFAULT NULL COMMENT '三供的id号',
  `manufacturer4_id` int(11) DEFAULT NULL COMMENT '四供的id号',
  PRIMARY KEY (`material_id`)
) ENGINE=InnoDB AUTO_INCREMENT=474 DEFAULT CHARSET=utf8;

